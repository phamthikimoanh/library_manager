#wapper-content
  %h1 Category list 
  %table.table.table-hover
    %thead
      %tr
        %th No.
        %th Name
        %th #
    %tbody#categories
      = render @categories
  = paginate @categories 

  %hr

  %button.btn.btn-default{:type => "button", :onclick => "addNew()"} Add New
  #form-add-new
    = render 'form'
